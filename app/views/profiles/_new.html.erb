<%= form_with model: @profile do |f| %>
  <%= f.hidden_field :user_id, :value => current_user.id %>
  <%= f.label "URL for your profile pic"%>
  <br>
  <%= f.text_field :profile_pic %>
  <br>
  <%= f.label "Tell us about yourself"%>
  <br>
  <%= f.text_area :about %>
  <br>
  <%= f.submit "Create Profile" %>
<% end %>